{
    "name": "bnoordsij/class-trees",
    "description": "This package visualizes class dependencies inside a project",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "Class"
    ],
    "authors": [
        {
            "name": "Bart Noordsij",
            "homepage": "https://github.com/bnoordsij"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "laravel/framework": "^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.20"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Bnoordsij\\ClassTrees\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bnoordsij\\ClassTrees\\Laravel\\ClassTreesServiceProvider"
            ]
        }
    }
}
